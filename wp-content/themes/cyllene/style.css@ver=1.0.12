	--black: #1A1A1A;
}
body {
	color: var(--black);
}
.flex {
	display: flex;
}
.align-items-center {
	align-items: center;
}
.justify-content-center {
	justify-content: center;
}
.justify-content-between {
	justify-content: space-between;
}
.justify-content-start {
	justify-content: flex-start;
}
@media (min-width: 1600) {
	.header-content-wrapper {
		max-width: 100%;
